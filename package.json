{
  "name": "integreat",
  "version": "0.8.0-alpha.0",
  "description": "Node.js integration layer",
  "author": "Kjell-Morten Bratsberg Thorsen <post@kjellmorten.no> (http://kjellmorten.no/)",
  "license": "ISC",
  "keywords": [
    "integreat",
    "integration",
    "data",
    "api"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run lint && npm run build && nyc --reporter=text-summary ava --config ./ava-dist.config.js",
    "test:inspect": "node --inspect node_modules/ava/profile.js",
    "test:watch": "npm run dev",
    "dev": "ava --watch",
    "build": "tsc",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "nyc:report": "nyc report --reporter=text --reporter=html",
    "lint": "eslint --ext .ts src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/integreat-io/integreat.git"
  },
  "bugs": {
    "url": "https://github.com/integreat-io/integreat/issues"
  },
  "homepage": "https://github.com/integreat-io/integreat#readme",
  "engines": {
    "node": ">= 8.6",
    "npm": ">= 5.6"
  },
  "dependencies": {
    "@types/later": "^1.2.6",
    "@types/nanoid": "^2.0.0",
    "debug": "^4.1.1",
    "got": "^9.6.0",
    "later": "^1.2.0",
    "map-any": "^0.2.1",
    "map-transform": "^0.3.6",
    "nanoid": "^2.1.11",
    "p-pipe": "^3.0.0",
    "ramda": "^0.27.0"
  },
  "devDependencies": {
    "@types/sinon": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "ava": "3.3.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "integreat-adapter-json": "^0.3.0",
    "nock": "^12.0.0",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "sinon": "^8.1.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
