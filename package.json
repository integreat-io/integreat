{
  "name": "integreat",
  "version": "0.7.27",
  "description": "Node.js integration layer",
  "author": "Kjell-Morten Bratsberg Thorsen <post@kjellmorten.no> (http://kjellmorten.no/)",
  "license": "ISC",
  "keywords": [
    "integreat",
    "integration",
    "data",
    "api"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test nyc --reporter=text-summary ava",
    "test:inspect": "node --inspect node_modules/ava/profile.js",
    "dev": "NODE_ENV=test ava --watch",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "nyc:report": "nyc report --reporter=text --reporter=html",
    "lint": "standard"
  },
  "ava": {
    "babel": true,
    "files": [
      "lib/**/*-test.js",
      "tests/**/*-test.js"
    ]
  },
  "nyc": {
    "exclude": [
      "**/*-test.js",
      "tests/helpers/**.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/integreat-io/integreat.git"
  },
  "bugs": {
    "url": "https://github.com/integreat-io/integreat/issues"
  },
  "homepage": "https://github.com/integreat-io/integreat#readme",
  "engines": {
    "node": ">= 8.6",
    "npm": ">= 5.6"
  },
  "dependencies": {
    "@sindresorhus/is": "^1.2.0",
    "debug": "^4.2.0",
    "got": "^9.6.0",
    "later": "^1.2.0",
    "map-any": "^0.2.1",
    "map-transform": "^0.3.11",
    "p-limit": "^2.3.0",
    "ramda": "^0.27.1",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "ava": "3.3.0",
    "coveralls": "^3.1.0",
    "dotenv": "^8.2.0",
    "integreat-adapter-json": "^0.2.1",
    "nock": "^12.0.3",
    "nyc": "^15.1.0",
    "sinon": "^9.0.3",
    "standard": "^14.3.4"
  }
}
