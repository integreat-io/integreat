  },
  "homepage": "https://github.com/integreat-io/integreat#readme",
  "engines": {
    "node": ">= 8.6",
    "npm": ">= 5.6"
  },
  "dependencies": {
    "@types/later": "^1.2.5",
    "@types/uuid": "^3.4.5",
    "debug": "^4.1.1",
    "got": "^9.6.0",
    "later": "^1.2.0",
    "map-any": "^0.2.1",
    "map-transform": "^0.3.6",
    "ramda": "^0.26.1",
    "uuid": "^3.2.2"
  },
